{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\ngrep '' $LFILE\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./grep '' $LFILE\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo grep '' $LFILE\n"
      }
    ]
  }
}
