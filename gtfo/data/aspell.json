{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\naspell -c \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./aspell -c \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo aspell -c \"$LFILE\"\n"
      }
    ]
  }
}
