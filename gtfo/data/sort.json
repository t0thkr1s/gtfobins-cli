{
  "functions": {
    "file-read": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsort -m \"$LFILE\"\n"
      }
    ],
    "file-write": [
      {
        "description": "",
        "code": "LFILE=file_to_write\necho DATA | sort -o \"$LFILE\"\n"
      }
    ],
    "suid": [
      {
        "description": "",
        "code": "LFILE=file_to_read\n./sort -m \"$LFILE\"\n"
      }
    ],
    "sudo": [
      {
        "description": "",
        "code": "LFILE=file_to_read\nsudo sort -m \"$LFILE\"\n"
      }
    ]
  }
}
