{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\ndialog --textbox \"$LFILE\" 0 0\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./dialog --textbox \"$LFILE\" 0 0\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo dialog --textbox \"$LFILE\" 0 0\n"
      }
    ]
  }
}
